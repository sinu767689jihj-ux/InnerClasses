public class StaticInnerClass{
int a=10;
static int b=20;
static class Inner{
void displayStaticMembers(){
//System.out.println("a="+a); error for static class/method not allow the non-static member access
System.out.println("b="+b);
}
}
public static void main(String[] args)
{
StaticInnerClass.Inner inner1=new StaticInnerClass.Inner();
inner1.displayStaticMembers();
}
}
